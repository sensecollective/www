{
	"extends": "airbnb",
  "rules": {
    "no-undef": 0,
    "no-return-assign": 0,
    "react/no-render-return-value": 0,
    "no-param-reassign": ["error", { "props": false }],
    "react/no-array-index-key": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": 0
  }
}
